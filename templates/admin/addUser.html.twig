{% extends 'base.html.twig' %}


{% block title %}Admin/Ajouter un utilisateur | {{ parent() }}{% endblock %}
{% block body %}
    <h3>Ajout d'un l'utilisateur</h3>
    <div class="card mt-4 text-center card-form " style="width: 25rem;">
        <div class="card-body">
            <p class="card-text">{{ form_start(registerForm) }}
                {{ form_widget(registerForm) }}</p>
            <button class="btn btn-primary" type="submit">Ajouter</button>
            <p class="card-text">{{ form_end(registerForm) }}</p>
        </div>
    </div>

{% endblock %}
